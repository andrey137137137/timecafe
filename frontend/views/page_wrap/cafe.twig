<div class="col-md-8 navbar-docs-push-down">
  <div class="panel panel-default-bg">
    <div class="panel-heading">
      <h2 class="page-header page-header-border-dark modernui-dark">
        <a href="/" class="hover-underline-none "><i class="icon-metro-arrow-left-3"></i></a> <span class="fa fa-list-alt"></span>
        {{ this.all_params.title }}
        {% if not(this.all_params.forAllCafe) %}
        {% if _can('ChooseCafe') %}
          - <font style="font-size: 28px;font-weight: 500;"><span class="title">{{ this.cafe.name }}</span></font>
          <a role="modal-remote" class="btn btn-science-blue ch_cafe" href="/change-cafe">
            <i class="fa fa-exchange"></i>
            {{ t('app','Change to view Anticafe') }}</a>
        {% endif %}
        {% endif %}
      </h2>
      <div class="clearfix"></div>
    </div>
    <div class="panel-body table-responsive">
      {{ content|raw }}
     </div>
  </div>
</div>
